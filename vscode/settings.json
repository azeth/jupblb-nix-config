{
    "window.zoomLevel": 4,
    "workbench.colorTheme": "Gruvbox Light Hard",
    "window.menuBarVisibility": "toggle",
    "editor.fontLigatures": true,
    "editor.fontFamily": "'PragmataPro Mono Liga'",
    "editor.minimap.enabled": false,
    "keyboard.dispatch": "keyCode",
    "editor.fontSize": 12,
    "editor.accessibilitySupport": "off",
    "editor.fastScrollSensitivity": 10,
    "editor.hover.delay": 200,
    "editor.mouseWheelScrollSensitivity": 5,
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": true,
    "files.maxMemoryForLargeFilesMB": 8192,
    "workbench.enableExperiments": false,
    "workbench.activityBar.visible": false,
    "zenMode.hideLineNumbers": false,
    "debug.console.fontSize": 16,
    "terminal.explorerKind": "external",
    "terminal.integrated.fontSize": 16,
    "http.proxySupport": "off",
    "git.allowForcePush": true,
    "markdown.preview.fontFamily": "-apple-system, Iosevka, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'Ubuntu', 'Droid Sans', sans-serif",
    "markdown.preview.fontSize": 16,
    "terminal.external.linuxExec": "kitty"
}